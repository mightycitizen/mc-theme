{% embed '@organisms/section/section-embed.twig' with {
  padding: 'tight',
  section_class: section_class | default,
  section_style: "narrow"
}
%}
  {% block section_content %}
    {% include '@molecules/cards/card/card-testimonial.twig' with testimonial
    %}

    {% for logo in logos %}
      <div class="c-logo-grid__item">
        {% if logo.url is empty %}<div class="u-duration-short border border-gray-30 c-logo-grid__item__logo
      bg-white">{% endif %}
          {% if logo.url %}<a class="u-duration-short border border-gray-30 c-logo-grid__item__logo
      bg-white" href="{{ logo.url }}">{% endif %}
            <img src="{{ logo.image.src }}" alt="{{ logo.image.alt }}" />
            {% if logo.url %}</a>{% endif %}
          {% if logo.url is empty %}</div>{% endif %}
      </div>
    {% endfor %}

  {% endblock %}
{% endembed %}
